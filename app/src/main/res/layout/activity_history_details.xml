<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
                                             xmlns:tool="http://schemas.android.com/tools"
                                             tool:context=".HistoryDetailsActivity"
                                             android:layout_height="match_parent">

    <fragment
            class="com.google.android.gms.maps.MapFragment"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/fragment"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/common_google_signin_btn_icon_dark"
            android:id="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/fragment" app:layout_constraintStart_toStartOf="parent"
    />
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/start"
            android:id="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/imageView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/finish"
            android:id="@+id/imageView5"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/time"
            android:id="@+id/imageView6"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/cost"
            android:id="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/imageView6" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"/>
    <TextView
            android:text="name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/vname" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/fragment" app:layout_constraintStart_toEndOf="@+id/imageView"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/id" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView3" app:layout_constraintStart_toEndOf="@+id/imageView"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/vname"/>
    <TextView
            android:text="START"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/start" app:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView3"/>
    <TextView
            android:text="coords"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/start_coords"
            app:layout_constraintTop_toBottomOf="@+id/start" android:layout_marginBottom="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView3" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView5"/>
    <TextView
            android:text="date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/date_start" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/start"/>
    <TextView
            android:text="time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/start_time" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/start_coords"/>
    <TextView
            android:text="END"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@+id/imageView5"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView5" android:id="@+id/textView9"/>
    <TextView
            android:text="coords"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/finish_coords" app:layout_constraintStart_toEndOf="@+id/imageView5"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView6"/>
    <TextView
            android:text="date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/finish_date" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView9"/>
    <TextView
            android:text="time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/finish_time" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/finish_coords"/>
    <TextView
            android:text="TIME"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@+id/imageView6"
            android:layout_marginStart="8dp" app:layout_constraintTop_toTopOf="@+id/imageView6"
            app:layout_constraintBottom_toBottomOf="@+id/imageView6" android:id="@+id/textView14"/>
    <TextView
            android:text="time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/total_time" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView6"
            app:layout_constraintTop_toTopOf="@+id/imageView6"/>
    <TextView
            android:text="PRICE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@+id/imageView7"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView7"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7"/>
    <TextView
            android:text="price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/price" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7"
            app:layout_constraintTop_toTopOf="@+id/imageView7"/>


</android.support.constraint.ConstraintLayout>